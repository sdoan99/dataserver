{
  "name": "websocket-aggregator",
  "version": "1.0.0",
  "description": "Dedicated WebSocket server aggregating multiple market data feeds",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docker:build": "docker build -t websocket-aggregator .",
    "docker:run": "docker run -p 8080:8080 websocket-aggregator"
  },
  "keywords": [
    "websocket",
    "trading",
    "market-data",
    "aggregator",
    "tradingview"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "cryptography": "^1.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "redis": "^4.6.11",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
